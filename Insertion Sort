#include <stdio.h>

int main()
{
    int a[7]={2,7,4,9,1,3,5};
    int key,pos,i;
    
    for(i=1; i<7; i++){
        key=a[i];
        pos=i;
        while(a[pos-1]>key && pos!=0){         //SHIFTING ELEMENT TO SORTED PART
            a[pos]=a[pos-1];
            pos--;
        }
        a[pos]=key;
    }
    
    for(i=0;i<7;i++){         //SORTED ARRAY
        printf("%d ",a[i]);
    }
    return 0;
}
